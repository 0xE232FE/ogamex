<script>

    var pendingForNextAction = false;
    var spyShortcutWait = false;
    var plunderShortcutWait = false;
    var destroyShortcutWait = false;
    var recylcerShortcutWait = false;

    function updateQueryStringParameter(uri, key, value) {

        console.log(uri);
        console.log(key);
        console.log(value);

      var re = new RegExp("([?&])" + key + "=.*?(&|$)", "i");
      var separator = uri.indexOf('?') !== -1 ? "&" : "?";
      if (uri.match(re)) {
        return uri.replace(re, '$1' + key + "=" + value + '$2');
      }
      else {
        return uri + separator + key + "=" + value;
      }
    }

    function LoadingGalaxyData() {
        $('.galaxy-content').append('<div style="position:absolute;width:100%;height:100%;background-color:rgba(255,255,255,0.1);text-align:center;line-height:550px;"><i class="fas fa-sync-alt fa-spin" style="font-size: 40px;"></i></div>');
    }

    var lastGalaxyData_X = 0;
    var lastGalaxyData_Y = 0;

    function GetGalaxyData() {

        var x = $('#galaxyInput').val();
        var y = $('#systemInput').val();

        if (x === undefined || x === null || x === '' || x <= 0)
            x = 1;

        if (y === undefined || y === null || y === '' || y <= 0)
            y = 1;

        if (lastGalaxyData_X === x && lastGalaxyData_Y === y)
            return;
        else {
            lastGalaxyData_X = x;
            lastGalaxyData_Y = y;
        }

        //LoadingGalaxyData();

        $('#galaxyContent').loading(true);


        $.ajax({
            url: '/galaxy/galaxydata?x=' + x + '&y=' + y,
            type: 'GET',
            cache:false,
            success: function (data) {
                $('#galaxyContent').loading(false);
                $('.galaxy-content').html(data);

                var newUrl = window.location.href.split('?')[0];
                newUrl = newUrl + '?x=' + x + '&y=' + y;

                var documentTitle = window.title;

                if (window.history.replaceState) {
                   //prevents browser from storing history with each change:
                   window.history.replaceState('', documentTitle, newUrl);
                }
            }
        });


    }

    function ShowPhalanx(planetId) {
        console.log(planetId);

        if (planetId === undefined || planetId === null || planetId === '')
            return;

        showAjaxModal('/galaxy/sensorReport?planetId=' + planetId, 'Sensor report', 'large', 500);
    }

    function SendPlunder(planetId,isStrong) {

        if (planetId == undefined || planetId == null) {
            return;
        }

        if (isStrong == undefined || isStrong == null) {
            isStrong = false;
        }

        if (plunderShortcutWait)
            return;

        plunderShortcutWait = true;

        var data = {
            Id: planetId
        };

            $('#galaxy-container').loading(true);
            PostRequest('/galaxy/sendplunder', JSON.stringify(data),
                function (res) {
                    if (res.IsSuccess) {
                        $('#galaxy-container').loading(false);
                        var id = GenerateGuid();
                        $('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item success">' + res.Message + '</div>');
                        setTimeout(function () {
                            $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        }, 2000);
                    }
                    else {
                        $('#galaxy-container').loading(false);
                        ShowError(res.Message);
                        //var id = GenerateGuid();
                        //$('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item fail">' + res.Message + '</div>');
                        //setTimeout(function () {
                        //    $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        //}, 2000);
                    }
                },
                function () {
                    $('#galaxy-container').loading(false);
                    ShowError();
                });

        setTimeout(function () { plunderShortcutWait = false; }, 500);
    }

    function SendDestroy(planetId,isStrong) {

        if (planetId == undefined || planetId == null) {
            return;
        }

        if (isStrong == undefined || isStrong == null) {
            isStrong = false;
        }

        if (destroyShortcutWait)
            return;

        destroyShortcutWait = true;

        var data = {
            Id: planetId
        };

            $('#galaxy-container').loading(true);
            PostRequest('/galaxy/senddestroy', JSON.stringify(data),
                function (res) {
                    if (res.IsSuccess) {
                        $('#galaxy-container').loading(false);
                        var id = GenerateGuid();
                        $('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item success">' + res.Message + '</div>');
                        setTimeout(function () {
                            $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        }, 2000);
                    }
                    else {
                        $('#galaxy-container').loading(false);
                        ShowError(res.Message);
                        //var id = GenerateGuid();
                        //$('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item fail">' + res.Message + '</div>');
                        //setTimeout(function () {
                        //    $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        //}, 2000);
                    }
                },
                function () {
                    $('#galaxy-container').loading(false);
                    ShowError();
                });

        setTimeout(function () { destroyShortcutWait = false; }, 500);
    }

    function SendSpy(x, y, z, pType,isStrong) {

        if (isStrong == undefined || isStrong == null) {
            isStrong = false;
        }

        if (spyShortcutWait)
            return;

        spyShortcutWait = true;

        var data = {
            X: x,
            Y: y,
            Z: z,
            PlanetType: pType
        };

            $('#galaxy-container').loading(true);
            PostRequest('/galaxy/sendspy', JSON.stringify(data),
                function (res) {
                    if (res.IsSuccess) {
                        $('#galaxy-container').loading(false);
                        var id = GenerateGuid();
                        $('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item success">' + res.Message + '</div>');
                        setTimeout(function () {
                            $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        }, 2000);
                    }
                    else {
                        $('#galaxy-container').loading(false);
                        ShowError(res.Message);
                        //var id = GenerateGuid();
                        //$('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item fail">' + res.Message + '</div>');
                        //setTimeout(function () {
                        //    $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                        //}, 2000);
                    }
                },
                function () {
                    $('#galaxy-container').loading(false);
                    ShowError();
                });

        setTimeout(function () { spyShortcutWait = false; }, 500);
    }

    function SendRecyclers(x, y, z) {
        //console.log('Recyclers sent to >> [' + x + ':' + y + ':' + z + ']');

        if (recylcerShortcutWait)
            return;

        recylcerShortcutWait = true;

        var data = {
            X: x,
            Y: y,
            Z: z,
        };

        $('#galaxy-container').loading(true);
        PostRequest('/galaxy/sendrecyclers', JSON.stringify(data),
            function (res) {
                if (res.IsSuccess) {
                    $('#galaxy-container').loading(false);
                    var id = GenerateGuid();
                    $('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item success">' + res.Message + '</div>');
                    setTimeout(function () {
                        $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                    }, 2000);
                }
                else {
                    $('#galaxy-container').loading(false);
                    ShowError(res.Message);
                    //var id = GenerateGuid();
                    //$('#galaxy-container .msg-box').append('<div id="' + id + '" class="msg-item fail">' + res.Message + '</div>');
                    //setTimeout(function () {
                    //    $('#' + id).fadeOut(1000, function () { $(this).remove(); });
                    //}, 2000);
                }
            },
            function () {
                $('#galaxy-container').loading(false);
                ShowError();
            });

        setTimeout(function () { recylcerShortcutWait = false; }, 500);

    }

    function SavePlanet(planetId) {
        console.log(planetId);
        console.log('Planet Saved.');

        var data = {
            Id: planetId
        };

        $('#galaxy-container').loading(true);
        PostRequest('/galaxy/saveplanet', JSON.stringify(data),
            function (res) {
                if (res.IsSuccess) {
                    $('#galaxy-container').loading(false);
                    ShowSuccessMessage("The planet has been saved.", null);
                }
                else {
                    $('#galaxy-container').loading(false);
                    ShowError(res.Message);
                }
            },
            function () {
                $('#galaxy-container').loading(false);
                ShowError();
            });

    }

    $(function () {

        window.localization.missileAttack = "Missile Attack";

        GetGalaxyData();

        $('.galaxy-route .route-input').focusin(function () {

            preventKeyup = true;

            var val = this.value;
            if (val === '0' || val === 0 || val == '0' || val == 0) {
                this.value = '';
            }
            else {
                if (/\D/g.test(this.value)) {
                    // Filter non-digits from input value.
                    this.value = this.value.replace(/\D/g, '');
                }
            }

            this.value = numberWithCommas(this.value);
        });

        $('.galaxy-route .route-input').focusout(function () {

            preventKeyup = false;

            var val = this.value;
            if (val === '' || val === ' ' || val == '' || val == ' ') {
                this.value = 1;
            }
            else {
                if (/\D/g.test(this.value)) {
                    // Filter non-digits from input value.
                    this.value = this.value.replace(/\D/g, '');
                }
            }

        });

        $('#galaxyInput').on("input", function (e) {

            if (/\D/g.test(this.value)) {
                // Filter non-digits from input value.
                this.value = this.value.replace(/\D/g, '');
            }

            var maxValue = 5;
            var minValue = 1;

            var intVal = parseInt(this.value);
            if (!isNaN(intVal)) {
                intVal = intVal % 10;
            }

            if (!isNaN(intVal)) {
                if (intVal < minValue) {
                    intVal = minValue;
                }
                else if (intVal > maxValue) {
                    intVal = maxValue;
                }

                this.value = intVal;
            }
        });

        $('#systemInput').on("input", function (e) {

            if (/\D/g.test(this.value)) {
                // Filter non-digits from input value.
                this.value = this.value.replace(/\D/g, '');
            }

            var maxValue = 499;
            var minValue = 1;

            var intVal = parseInt(this.value);
            if (!isNaN(intVal)) {
                if (intVal < minValue) {
                    intVal = minValue;
                }
                else if (intVal > maxValue) {
                    intVal = maxValue;
                }
                this.value = intVal;
            }


            if (e.keyCode === 13) {
                GetGalaxyData();
            }
        });

        $('#galaxyInput').on('keyup', function (e) {
             if (e.keyCode === 13) {
                GetGalaxyData();
            }
        });

        $('#systemInput').on('keyup', function (e) {
             if (e.keyCode === 13) {
                GetGalaxyData();
            }
        });

        $('.galaxy-route .left-arrow').click(function () {

            if (preventKeyup)
                return;

            

            preventKeyup = true;
            setTimeout(function () {
                preventKeyup = false;
            }, 300);

            var input = $(this).closest('.route-item').find('.route-input');

            var value = input.val();
            if (value == undefined || value == null || value === '' || value === ' ') {
                input.val(1);
            }
            else {
                var intVal = parseInt(value);
                if (!isNaN(intVal)) {
                    intVal -= 1;
                    input.val(intVal);
                }
            }

            input.trigger('input');

            GetGalaxyData();

            return false;
        });

        $('.galaxy-route .right-arrow').click(function () {

            if (preventKeyup)
                return;

            

            preventKeyup = true;
            setTimeout(function () {
                preventKeyup = false;
            }, 300);

            var input = $(this).closest('.route-item').find('.route-input');

            var value = input.val();
            if (value == undefined || value == null || value === '' || value === ' ') {
                input.val(999);
            }
            else {
                var intVal = parseInt(value);
                if (!isNaN(intVal)) {
                    intVal += 1;
                    input.val(intVal);
                }
            }

            input.trigger('input');

            GetGalaxyData();

            return false;
        });

        var preventKeyup = false;

        $("body").keyup(function (e) {

            console.log(e.keyCode);

            if (e.keyCode == 37) {
                // left
                $('#btnSystemLeft').click();
            }
            else if (e.keyCode == 39) {
                // right
                $('#btnSystemRight').click();
            }
            else if (e.keyCode == 38) {
                // up
                $('#btnGalaxyRight').click();
            }
            else if (e.keyCode == 40) {
                // down
                $('#btnGalaxyLeft').click();
            }
        });

        $('.x-btn-go').click(function () {
            GetGalaxyData();
            return false;
        })

        setInterval(function () {
            var asteroidObj = $('#galaxy-container .galaxy-info [data-asteroid-disappear]');
            if (asteroidObj != undefined && asteroidObj != null) {

                var seconds = parseInt(asteroidObj.attr('data-asteroid-disappear'));
                seconds -= 1;
                if (seconds <= 0)
                    seconds = 0;

                asteroidObj.html('(' + ConvertToTimeString(seconds) + ')');
                asteroidObj.attr('data-asteroid-disappear', seconds);
            }
        }, 1000);


    //    window.MouseXCoord = 0;
    //    window.MouseYCoord = 0;

    //    document.onmousemove = handleMouseMove;
