<script>

        var _fleetReturnCounterInterval = null;

        function ReloadPage() {
            if (!window.location.hash) {
                window.location.href = window.location.href;
            } else {
                window.location.reload();
            }
        }

        function ReportChatMessage(msgId,isUniverseMessage) {

            var data = {
                Id: msgId
            };

            if (isUniverseMessage != undefined && isUniverseMessage != null && isUniverseMessage === true) {
                PostRequest('/chat/reportuniversalmessage', JSON.stringify(data),
                function (res) {
                    if (res.IsSuccess) {
                        ShowSuccessMessage(`Your report has been sent to administration for review.`);
                    }
                    else {
                        ShowError(res.Message);
                    }
                },
                function () {
                    ShowError();
                });
            }
            else {
                PostRequest('/chat/reportmessage', JSON.stringify(data),
                function (res) {
                    if (res.IsSuccess) {
                        ShowSuccessMessage(`Your report has been sent to administration for review.`);
                    }
                    else {
                        ShowError(res.Message);
                    }
                },
                function () {
                    ShowError();
                });
            }
        }

        function ShowError(contentHtml) {
            var defaultMsg = `An unexpected error occurred.`;
            var errorMsg = '';
            if (contentHtml === undefined || contentHtml == null || contentHtml === '') {
                errorMsg = defaultMsg;
            }
            else
                errorMsg = contentHtml;

            Swal.fire({
                type: 'error',
                title: `Error`,
                html: `<div style="font-size:12px;">`+errorMsg+`</div>`,
                footer: '',
                onBeforeOpen: function (ele) {
                    console.log($(ele));
                    $(ele).addClass('swal2-modal-error');
                }
            });
        }

        function ShowNotEnoughDM() {
            var message = `Not enough Dark Matter available!</br> Do you want to buy some now?`;

            Swal.fire({
                title: 'Error',
                html: '<span style="font-size: 12px; ">' + message + '</span>',
                type: 'error',
                showCancelButton: true,
                buttonStyling: false,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes',
                cancelButtonText: 'No',
                onBeforeOpen: function (ele) {
                    console.log($(ele));
                    $(ele).addClass('swal2-modal-error');
                }
            }).then(function(result){
                if (result.value) {
                    window.location.href = '/darkmatter/purchase';
                }
            })
        }

        function ShowSuccessMessage(message, callback) {
            Swal.fire({
                title: `Success`,
                html: `<span style="font-size:12px;">${message}</span>`,
                confirmButtonColor: '#3085d6',
                confirmButtonText: 'Ok',
                onBeforeOpen: function (ele) {
                    console.log($(ele));
                    $(ele).addClass('swal2-modal-success');
                }
            }).then((result) => {
                if (result.value) {
                    if (callback != undefined && callback != null) {
                        callback();
                    }
                }
            });
        }

        window.isConnectedChatWs = false;

        function ConnectChatWS() {
            if (window.isConnectedChatWs)
                return;

            var hub = $.connection.wSHub;

            hub.client.newChatMessage = function (dataJson) {
                var data = JSON.parse(dataJson);
                console.log(data);

                data.MessageContent = Emojify(data.MessageContent);

                if (data.IsAllianceChat === true) {
                    var allianceChatMessagesContainer = $('#alliance_chat_messages');
                    if (allianceChatMessagesContainer.length > 0) {
                        var msgDateObj = new Date(data.MessageDate);

                        allianceChatMessagesContainer.append('<div class="message-item"><span class="msg-sender">' + data.FromPlayerName + '</span><span class="msg-date">' + FormatDate(msgDateObj) + '</span><span class="msg-arrow"></span><div class="msg-content">' + data.MessageContent + '</div></div>');

                       allianceChatMessagesContainer.animate({ scrollTop: allianceChatMessagesContainer.prop("scrollHeight")}, 1000);
                    }
                }
                else {
                    var playerChatMessagesContainer = $('#player_chat_messages_' + data.FromPlayerId);
                    if (playerChatMessagesContainer.length > 0) {

                        var msgDateObj = new Date(data.MessageDate);

                        playerChatMessagesContainer.append('<div class="message-item"><span class="msg-sender">' + data.FromPlayerName + '</span><span class="msg-date">' + FormatDate(msgDateObj) + '</span><span class="msg-arrow"></span><div class="msg-content">' + data.MessageContent + '</div></div>');

                       playerChatMessagesContainer.animate({ scrollTop: playerChatMessagesContainer.prop("scrollHeight")}, 1000);

                    }
                }

            };

            $.connection.hub.start();
            window.isConnectedChatWs = true;
        }

        

        $(function () {

            window.fleetDetailTableOpened = false;

            var currentMissionRemainingSeconds = 57;

            setInterval(function () {
                currentMissionRemainingSeconds -= 1;
                $('#NextMissionRemainingTime').html(ConvertToTimeString(currentMissionRemainingSeconds));
            }, 1000);


            $('#fleet-movement-detail-btn').click(function () {

                if (window.fleetDetailTableOpened === true) {
                    $('#layoutFleetMovements').toggleClass('opened');
                }
                else {

                    $('#header .fleet-info-section').loading(true);

                    $.ajax({
                        url: '/home/fleetmovementlist',
                        type: 'GET',
                        cache:false,
                        success: function (data) {
                            window.fleetDetailTableOpened = true;

                            $('#fleet-movement-content').html(data);

                            ConfigureModals();
                            ConfigureTooltips();

                            $('#layoutFleetMovements').toggleClass('opened');

                            $('#header .fleet-info-section').loading(false);
                        }
                    });
                }

                return false;
            })

        });


        


    </script>

</body></html>